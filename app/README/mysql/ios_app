create table ios_app (id int primary key not null auto_increment,app_id char(37),device_id char(33),device_type int) default charset=utf8;
                                          # Create ios_app table
desc ios_app;                             # Look at the ios_app table
drop table ios_app;                       # Delete a table "ios_app"
select * from ios_app;                    # Look at the ios_app table context 
